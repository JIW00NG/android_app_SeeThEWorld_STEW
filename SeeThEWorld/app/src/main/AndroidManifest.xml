<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jbnu.moblie.app.one.team.STEW">


    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        android:maxSdkVersion="18" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"
        android:maxSdkVersion="18" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="jbnu.moblie.app.one.team.STEW.TextRecogniserActivity"
            android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize">

        </activity>
        <activity android:name="jbnu.moblie.app.one.team.STEW.SignUp">

        </activity>
        <activity android:name="jbnu.moblie.app.one.team.STEW.RecognisedRecordActivity">

        </activity>
        <activity android:name="jbnu.moblie.app.one.team.STEW.Record">

        </activity>
        <activity android:name="jbnu.moblie.app.one.team.STEW.Loading">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="jbnu.moblie.app.one.team.STEW.Login">
        </activity>
        <service android:name=".UploadToStorage">

        </service>
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" /> <!--version--><!-- make sure added-->
        <meta-data
            android:name="com.google.mlkit.vision.DEPENDENCIES"
            android:value="ocr" />
        <!-- To use multiple models: android:value="ocr,model2,model3" -->
    </application>

</manifest>